class ComboItem {
  public static void reinitializeAutouniqueID(List<Plane> planes) {
    nextId = 0; 
    for (Plane plane : planes) {
      if (plane.getId() > nextId) {
        nextId = plane.getId();
      }
    }
    nextId++;
  }
  
