class BookableItem {
  public static void reinitializeUniqueFlightNumber(List<Flight> flights) {
    flightsByFlightNumber.clear();
    for (var f : flights) {
      flightsByFlightNumber.put(f.getFlightNumber(), f);
    }
  }
}
